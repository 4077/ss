<div class="{__NODE_ID__}" instance="{__INSTANCE__}">

    <!-- if row_1 -->
    <div class="row_1">
        <div class="types">
            <!-- type -->
            {BUTTON}
            <!-- / -->
        </div>

        <div class="buttons">
            {SAVE_ALL_BUTTON}
        </div>
    </div>
    <!-- / -->

    <div class="row_2">
        <div class="row_2_1">
            <div class="mode_buttons">
                {ENABLED_BUTTON}
                {REWRITE_BUTTON}
            </div>
            <div class="buttons">
                <div class="update_css_button">
                    <div class="icon fa fa-refresh"></div>
                </div>
                {RESET_BUTTON}
                {SAVE_BUTTON}
            </div>
        </div>
        <!-- dev -->
        <div class="row_2_2">
            <a class="node_file" href="{IDEA_URL}">
                {NODE_PATH}
            </a>
            <a class="path" href="{NODE_URL}" target="_blank">
                <span class="icon fa fa-external-link"></span>
            </a>
        </div>
        <!-- / -->
    </div>

</div>
