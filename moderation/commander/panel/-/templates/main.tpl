<div class="{__NODE_ID__} focusable {FOCUS_CLASS}" instance="{__INSTANCE__}">

    <div class="a">
        <div class="status_filter">
            <!-- status_filter -->
            {BUTTON}
            <!-- / -->
        </div>
        <div class="paginator">
            {PAGINATOR}
        </div>
    </div>

    <div class="b">
        <table>
            <!-- product -->
            <tr class="product row {INSTALLED_CLASS} {STATUS_CLASS}" n="{N}" product_id="{PRODUCT_ID}">
                <td class="icon {STATUS_CLASS}" title="{STATUS_TITLE}">
                    <div class="fa {ICON_CLASS}"></div>
                </td>
                <td class="date" width="1">{DATE}</td>
                <td class="name">{NAME}</td>
            </tr>
            <!-- / -->
        </table>
    </div>

</div>
