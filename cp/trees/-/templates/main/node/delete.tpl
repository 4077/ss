<div class="{__NODE_ID__}" instance="{__INSTANCE__}">

    <div class="warning">ВНИМАНИЕ!</div>

    <div class="label">Будет удалено:</div>

    <!-- cats -->
    <div class="row">
        <div class="label">Категорий: </div>
        <div class="value">{COUNT}</div>
    </div>
    <!-- / -->

    <!-- products -->
    <div class="row">
        <div class="label">Товаров:</div>
        <div class="value">{COUNT}</div>
    </div>
    <!-- / -->

    <!-- refs -->
    <div class="row">
        <div class="label">Ссылающихся товаров в других ветках:</div>
        <div class="value">{COUNT}</div>
    </div>
    <!-- / -->

    <div class="buttons">
        {DELETE_BUTTON}
        {CANCEL_BUTTON}
    </div>

</div>
