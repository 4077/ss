            <div class="icon {REQUEST_ICON_CLASS}"></div>
            <div class="label">ожидается от</div>
            <div class="user_select">
                {USER_SELECT}
            </div>
            {CANCEL_BUTTON}
        </div>
        <!-- request/images -->
        <div class="images">
            <!-- request/images/image -->
            <div class="image">
                {IMAGE}
                <div class="buttons">
