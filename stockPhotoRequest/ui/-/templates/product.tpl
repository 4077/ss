            </div>
            <!-- / -->
        </div>
        <!-- / -->
    </div>
    <!-- / -->

    <!-- if not request -->
    <div class="create_panel">
        {CREATE_BUTTON}
        <div class="user_select">
            {USER_SELECT}
        </div>
    </div>
    <!-- / -->

</div>
